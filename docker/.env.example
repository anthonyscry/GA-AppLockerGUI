# GA-AppLockerGUI Docker Environment Configuration
# Copy this file to .env and configure your values
# NEVER commit the .env file to version control

# Domain Configuration
DOMAIN_NAME=applocker.local
DOMAIN_NETBIOS=APPLOCKER

# Credentials - CHANGE THESE IN PRODUCTION
# Use strong, unique passwords
DOMAIN_ADMIN_PASSWORD=ChangeMe123!
SAFE_MODE_PASSWORD=ChangeMe456!

# Network Configuration
DNS_FORWARDER=8.8.8.8

# Application Configuration
NODE_ENV=development

# Certificate Configuration (optional)
# Comma-separated list of allowed certificate fingerprints
# ALLOWED_CERT_FINGERPRINTS=sha256:abc123...,sha256:def456...
